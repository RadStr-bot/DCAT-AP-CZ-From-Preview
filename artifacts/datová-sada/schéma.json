{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://ofn.gov.cz/dcat-ap-cz-360-2023/2025-08-12/datová-sada/schéma.json",
  "type": "object",
  "required": [
    "@context",
    "iri",
    "typ",
    "název",
    "popis",
    "klíčové_slovo",
    "je_zahrnuta_v_isvs",
    "právní_předpis"
  ],
  "properties": {
    "@context": {
      "oneOf": [
        {
          "const": "https://ofn.gov.cz/dcat-ap-cz-360-2023/2025-08-12/datová-sada/kontext.jsonld"
        },
        {
          "type": "array",
          "contains": {
            "const": "https://ofn.gov.cz/dcat-ap-cz-360-2023/2025-08-12/datová-sada/kontext.jsonld"
          },
          "items": {
            "type": "string",
            "format": "iri"
          }
        }
      ]
    },
    "iri": {
      "type": "string",
      "format": "iri",
      "pattern": "^https://.*$"
    },
    "typ": {
      "oneOf": [
        {
          "const": null
        },
        {
          "type": "array",
          "contains": {
            "const": null
          },
          "items": {
            "type": "string"
          }
        }
      ]
    },
    "název": {
      "title": "název",
      "description": "Název datové sady",
      "type": "object",
      "additionalProperties": {
        "title": "Hodnota v jiném jazyce",
        "type": "string"
      },
      "properties": {
        "cs": {
          "title": "Hodnota v českém jazyce",
          "type": "string"
        },
        "en": {
          "title": "Hodnota v anglickém jazyce",
          "type": "string"
        }
      }
    },
    "popis": {
      "title": "popis",
      "description": "Volný text s popisem datové sady",
      "type": "object",
      "additionalProperties": {
        "title": "Hodnota v jiném jazyce",
        "type": "string"
      },
      "properties": {
        "cs": {
          "title": "Hodnota v českém jazyce",
          "type": "string"
        },
        "en": {
          "title": "Hodnota v anglickém jazyce",
          "type": "string"
        }
      }
    },
    "poskytovatel": {
      "title": "poskytovatel",
      "description": "Poskytovatel datové sady",
      "type": "string",
      "pattern": "^ovm:[^/]+$",
      "examples": [
        "ovm:66003008"
      ]
    },
    "téma": {
      "description": "Téma datové sady. Datová sada může být popsána více tématy.",
      "type": "array",
      "items": {
        "type": "string",
        "pattern": "^témata:.*$",
        "examples": [
          "témata:TRAN"
        ]
      }
    },
    "periodicita_aktualizace": {
      "title": "periodicita aktualizace",
      "description": "Frekvence, se kterou je datová sada aktualizována.",
      "type": "string",
      "pattern": "^frekvence:.*$",
      "examples": [
        "frekvence:MONTHLY"
      ]
    },
    "klíčové_slovo": {
      "title": "Text",
      "type": "object",
      "additionalProperties": {
        "title": "Hodnota v jiném jazyce",
        "type": "array",
        "items": {
          "type": "string"
        }
      },
      "properties": {
        "cs": {
          "title": "Hodnota v českém jazyce",
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "en": {
          "title": "Hodnota v anglickém jazyce",
          "type": "array",
          "items": {
            "type": "string"
          }
        }
      }
    },
    "časové_pokrytí": {
      "title": "časové pokrytí",
      "description": "Časový úsek pokrytý datovou sadou",
      "type": "object",
      "properties": {
        "začátek": {
          "title": "Datum",
          "description": "Začátek doby trvání.",
          "type": "string",
          "format": "date"
        },
        "konec": {
          "title": "Datum",
          "type": "string",
          "format": "date"
        }
      }
    },
    "kontaktní_bod": {
      "title": "Organizace",
      "description": "Kontaktní informace, které mohou být využity pro zasílání připomínek k datové sadě.",
      "type": "object",
      "required": [
        "typ",
        "jméno",
        "e-mail"
      ],
      "properties": {
        "iri": {
          "type": "string",
          "format": "iri"
        },
        "typ": {
          "oneOf": [
            {
              "const": "Organizace"
            },
            {
              "type": "array",
              "contains": {
                "const": "Organizace"
              },
              "items": {
                "type": "string"
              }
            }
          ]
        },
        "jméno": {
          "title": "jméno",
          "description": "Jméno objektu jako formátovaný text.",
          "type": "object",
          "additionalProperties": {
            "title": "Hodnota v jiném jazyce",
            "type": "string"
          },
          "required": [
            "cs"
          ],
          "properties": {
            "cs": {
              "title": "Hodnota v českém jazyce",
              "type": "string"
            },
            "en": {
              "title": "Hodnota v anglickém jazyce",
              "type": "string"
            }
          }
        },
        "e-mail": {
          "title": "email",
          "description": "E-mailová adresa pro komunikaci.",
          "type": "string",
          "format": "iri"
        }
      }
    },
    "dokumentace": {
      "title": "dokumentace",
      "description": "Stránka či dokument o datové sadě",
      "type": "string",
      "format": "iri",
      "pattern": "^https://.*$",
      "examples": [
        "https://www.mdcr.cz/Dokumenty/Verejna-doprava/Jizdni-rady,-kalendare-pro-jizdni-rady,-metodi-(1)/Jizdni-rady-verejne-dopravy"
      ]
    },
    "specifikace": {
      "title": "specifikace",
      "description": "Specifikace, jíž se datová sada řídí.",
      "type": "array",
      "items": {
        "type": "string",
        "format": "iri",
        "pattern": "^https://.*$",
        "examples": [
          "https://ofn.gov.cz/turistické-cíle/2020-07-01/"
        ]
      }
    },
    "koncept_euroVoc": {
      "title": "klasifikace dle EuroVoc",
      "description": "Kategorie či téma datové sady dle EuroVoc.",
      "type": "array",
      "items": {
        "type": "string",
        "pattern": "^euroVoc:.*$",
        "examples": [
          "euroVoc:1001"
        ]
      }
    },
    "týká_se_pojmu": {
      "title": "týká se pojmu",
      "type": "array",
      "items": {
        "type": "string",
        "format": "iri",
        "pattern": "^https://.*$",
        "examples": [
          "https://slovník.gov.cz/legislativní/sbírka/361/2000/pojem/řidič"
        ]
      }
    },
    "je_zahrnuta_v_isvs": {
      "type": "string",
      "pattern": "^isvs:[^/]+$",
      "examples": [
        "isvs:3"
      ]
    },
    "je_součástí": {
      "title": "je součástí",
      "description": "Série datových sad, jíž je datová sada součástí.",
      "type": "string",
      "format": "iri",
      "pattern": "^https://.*$"
    },
    "právní_předpis": {
      "title": "příslušný právní předpis",
      "description": "Indikace příslušného právního předpisu předepisujícího vznik datové sady.",
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "string",
        "format": "iri"
      }
    },
    "distribuce": {
      "type": "array",
      "items": {
        "anyOf": [
          {
            "title": "Distribuce - Soubor ke stažení",
            "description": "Konkrétní reprezentace datové sady jako souboru ke stažení.",
            "type": "object",
            "required": [
              "iri",
              "typ",
              "přístupové_url",
              "právní_předpis",
              "podmínky_užití",
              "způsob_získání_sdílených_údajů",
              "způsob_sdílení_údajů",
              "typ_obsahu_sdílených_údajů",
              "formát",
              "typ_média"
            ],
            "properties": {
              "iri": {
                "type": "string",
                "format": "iri",
                "pattern": "^https://.*$"
              },
              "typ": {
                "oneOf": [
                  {
                    "const": "Distribuce - Soubor ke stažení"
                  },
                  {
                    "type": "array",
                    "contains": {
                      "const": "Distribuce - Soubor ke stažení"
                    },
                    "items": {
                      "type": "string"
                    }
                  }
                ]
              },
              "název": {
                "title": "název",
                "description": "Název distribuce datové sady",
                "type": "object",
                "additionalProperties": {
                  "title": "Hodnota v jiném jazyce",
                  "type": "string"
                },
                "properties": {
                  "cs": {
                    "title": "Hodnota v českém jazyce",
                    "type": "string"
                  },
                  "en": {
                    "title": "Hodnota v anglickém jazyce",
                    "type": "string"
                  }
                }
              },
              "přístupové_url": {
                "title": "přístupové URL",
                "type": "string",
                "format": "iri",
                "pattern": "^https://.*$",
                "examples": [
                  "https://www.trebic.cz/zdroj/aktualita/2020/dopadení-lupiče-na-francouzské-ulici"
                ]
              },
              "právní_předpis": {
                "title": "příslušný právní předpis",
                "description": "Indikace příslušného právního předpisu předepisujícího vznik distribuce.",
                "type": "array",
                "minItems": 1,
                "items": {
                  "type": "string",
                  "format": "iri"
                }
              },
              "podmínky_užití": {
                "type": "object",
                "properties": {
                  "autorské_dílo": {
                    "title": "autorské dílo",
                    "type": "string",
                    "format": "iri",
                    "examples": [
                      "https://data.gov.cz/podmínky-užití/neobsahuje-autorská-díla/",
                      "https://data.gov.cz/podmínky-užití/není-autorskoprávně-chráněnou-databází/",
                      "https://data.gov.cz/podmínky-užití/není-chráněna-zvláštním-právem-pořizovatele-databáze/",
                      "https://creativecommons.org/licenses/by/4.0/",
                      "https://creativecommons.org/publicdomain/zero/1.0/",
                      "https://data.gov.cz/podmínky-užití/neobsahuje-osobní-údaje/",
                      "https://data.gov.cz/podmínky-užití/obsahuje-osobní-údaje/"
                    ]
                  },
                  "autor": {
                    "title": "jména autorů díla",
                    "type": "object",
                    "additionalProperties": {
                      "title": "Hodnota v jiném jazyce",
                      "type": "string"
                    },
                    "properties": {
                      "cs": {
                        "title": "Hodnota v českém jazyce",
                        "type": "string"
                      },
                      "en": {
                        "title": "Hodnota v anglickém jazyce",
                        "type": "string"
                      }
                    }
                  },
                  "databáze_jako_autorské_dílo": {
                    "title": "originální databáze",
                    "type": "string",
                    "format": "iri",
                    "examples": [
                      "https://data.gov.cz/podmínky-užití/neobsahuje-autorská-díla/",
                      "https://data.gov.cz/podmínky-užití/není-autorskoprávně-chráněnou-databází/",
                      "https://data.gov.cz/podmínky-užití/není-chráněna-zvláštním-právem-pořizovatele-databáze/",
                      "https://creativecommons.org/licenses/by/4.0/",
                      "https://creativecommons.org/publicdomain/zero/1.0/",
                      "https://data.gov.cz/podmínky-užití/neobsahuje-osobní-údaje/",
                      "https://data.gov.cz/podmínky-užití/obsahuje-osobní-údaje/"
                    ]
                  },
                  "autor_databáze": {
                    "title": "jména autorů originální databáze",
                    "type": "object",
                    "additionalProperties": {
                      "title": "Hodnota v jiném jazyce",
                      "type": "string"
                    },
                    "properties": {
                      "cs": {
                        "title": "Hodnota v českém jazyce",
                        "type": "string"
                      },
                      "en": {
                        "title": "Hodnota v anglickém jazyce",
                        "type": "string"
                      }
                    }
                  },
                  "databáze_chráněná_zvláštními_právy": {
                    "title": "zvláštní právo pořizovatele databáze",
                    "type": "string",
                    "format": "iri",
                    "examples": [
                      "https://data.gov.cz/podmínky-užití/neobsahuje-autorská-díla/",
                      "https://data.gov.cz/podmínky-užití/není-autorskoprávně-chráněnou-databází/",
                      "https://data.gov.cz/podmínky-užití/není-chráněna-zvláštním-právem-pořizovatele-databáze/",
                      "https://creativecommons.org/licenses/by/4.0/",
                      "https://creativecommons.org/publicdomain/zero/1.0/",
                      "https://data.gov.cz/podmínky-užití/neobsahuje-osobní-údaje/",
                      "https://data.gov.cz/podmínky-užití/obsahuje-osobní-údaje/"
                    ]
                  },
                  "osobní_údaje": {
                    "title": "osobní údaje",
                    "type": "string",
                    "format": "iri",
                    "examples": [
                      "https://data.gov.cz/podmínky-užití/neobsahuje-autorská-díla/",
                      "https://data.gov.cz/podmínky-užití/není-autorskoprávně-chráněnou-databází/",
                      "https://data.gov.cz/podmínky-užití/není-chráněna-zvláštním-právem-pořizovatele-databáze/",
                      "https://creativecommons.org/licenses/by/4.0/",
                      "https://creativecommons.org/publicdomain/zero/1.0/",
                      "https://data.gov.cz/podmínky-užití/neobsahuje-osobní-údaje/",
                      "https://data.gov.cz/podmínky-užití/obsahuje-osobní-údaje/"
                    ]
                  }
                }
              },
              "sdílí_údaj": {
                "title": "zprostředkovává sdílení",
                "type": "array",
                "items": {
                  "title": "Sdílení údaje",
                  "type": "object",
                  "required": [
                    "typ",
                    "odpovídající_pojem",
                    "způsob_získání_sdílených_údajů",
                    "způsob_sdílení_údajů",
                    "typ_obsahu_sdílených_údajů"
                  ],
                  "properties": {
                    "typ": {
                      "oneOf": [
                        {
                          "const": "Sdílení údaje"
                        },
                        {
                          "type": "array",
                          "contains": {
                            "const": "Sdílení údaje"
                          },
                          "items": {
                            "type": "string"
                          }
                        }
                      ]
                    },
                    "odpovídající_pojem": {
                      "title": "odpovídající pojem",
                      "type": "string",
                      "format": "iri",
                      "pattern": "^https://.*$",
                      "examples": [
                        "https://slovník.gov.cz/legislativní/sbírka/361/2000/pojem/řidič"
                      ]
                    },
                    "způsob_získání_sdílených_údajů": {
                      "title": "je získán způsobem",
                      "description": "Orgán veřejné správy strukturuje data vedená v informačním systému na údaje následujících kategorií: a) základních registrů: údaje čerpané pro výkon agendy ze základních registrů b) jiných agend: údaje získávané pro výkon agendy od jiných úřadů c) vlastní: údaje vznikající při výkonu agendy, které mají evidenční charakter, tedy vztahují se k objektu nebo subjektu práva za účelem výkonu veřejné správy d) provozní: údaje vznikající při výkonu agendy, které nemají evidenční charakter a nevznikají za účelem výkonu veřejné správy",
                      "type": "string",
                      "pattern": "^způsoby-získání:[^/]+$",
                      "examples": [
                        "způsoby-získání:základních-registrů",
                        "způsoby-získání:jiných-agend",
                        "způsoby-získání:vlastní",
                        "způsoby-získání:provozní"
                      ]
                    },
                    "způsob_sdílení_údajů": {
                      "title": "je sdílen způsobem",
                      "description": "Orgán veřejné správy strukturuje data vedená v informačním systému podle způsobu jejich sdílení na údaje:  a) veřejně přístupné: údaje publikované veřejně ve formě otevřených dat či jakkoliv jinak b) poskytované na žádost: údaje poskytované na žádost subjektu práva nebo na základě zákona o svobodném přístupu k informacím c) zpřístupňované pro výkon agendy: údaje sdílené jiným agendám, které mají oprávněný zájem je využívat pro výkon své působnosti d) nesdílené: údaje, které nejsou sdílené žádným z předchozích způsobů",
                      "type": "string",
                      "pattern": "^způsoby-sdílení:[^/]+$",
                      "examples": [
                        "způsoby-sdílení:veřejně-přístupné",
                        "způsoby-sdílení:poskytované-na-žádost",
                        "způsoby-sdílení:zpřístupňované-pro-výkon-agendy",
                        "způsoby-sdílení:nesdílené"
                      ]
                    },
                    "typ_obsahu_sdílených_údajů": {
                      "title": "je sdílen jako",
                      "description": "Orgán veřejné správy strukturuje data vedená v informačním systému podle jejich obsahu na údaje:  a) identifikační: údaje, které, které samostatně nebo ve skupině identifikují konkrétní subjekt práva  b) evidenční: údaje, které neidentifikují konkrétní subjekt práva, ale váží se k subjektu nebo objektu práva v rámci agendy a evidence, která se v ní vede  c) statistické: údaje, které seskupují a formou anonymizace či pseudonymizace zamezují vazbě konkrétní subjekt práva",
                      "type": "string",
                      "pattern": "^typy-obsahu:[^/]+$",
                      "examples": [
                        "typy-obsahu:identifikační",
                        "typy-obsahu:evidenční",
                        "typy-obsahu:statistické"
                      ]
                    }
                  }
                }
              },
              "způsob_získání_sdílených_údajů": {
                "title": "má způsob získání dat sdílených rozhraním",
                "description": "Orgán veřejné správy strukturuje data vedená v informačním systému na údaje následujících kategorií: a) základních registrů: údaje čerpané pro výkon agendy ze základních registrů b) jiných agend: údaje získávané pro výkon agendy od jiných úřadů c) vlastní: údaje vznikající při výkonu agendy, které mají evidenční charakter, tedy vztahují se k objektu nebo subjektu práva za účelem výkonu veřejné správy d) provozní: údaje vznikající při výkonu agendy, které nemají evidenční charakter a nevznikají za účelem výkonu veřejné správy",
                "type": "array",
                "minItems": 1,
                "items": {
                  "type": "string",
                  "pattern": "^způsoby-získání:[^/]+$",
                  "examples": [
                    "způsoby-získání:základních-registrů",
                    "způsoby-získání:jiných-agend",
                    "způsoby-získání:vlastní",
                    "způsoby-získání:provozní"
                  ]
                }
              },
              "způsob_sdílení_údajů": {
                "title": "má způsob sdílení rozhraním",
                "description": "Orgán veřejné správy strukturuje data vedená v informačním systému podle způsobu jejich sdílení na údaje:  a) veřejně přístupné: údaje publikované veřejně ve formě otevřených dat či jakkoliv jinak b) poskytované na žádost: údaje poskytované na žádost subjektu práva nebo na základě zákona o svobodném přístupu k informacím c) zpřístupňované pro výkon agendy: údaje sdílené jiným agendám, které mají oprávněný zájem je využívat pro výkon své působnosti d) nesdílené: údaje, které nejsou sdílené žádným z předchozích způsobů",
                "type": "string",
                "pattern": "^způsoby-sdílení:[^/]+$",
                "examples": [
                  "způsoby-sdílení:veřejně-přístupné",
                  "způsoby-sdílení:poskytované-na-žádost",
                  "způsoby-sdílení:zpřístupňované-pro-výkon-agendy",
                  "způsoby-sdílení:nesdílené"
                ]
              },
